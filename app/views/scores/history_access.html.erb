<div id="history-access">
  <div>
    <table width="100%" cellpadding="0" cellspacing="0" border="0">
      <tbody>
        <% @history.each_with_index do |hist, idx| %>
          <tr class="<%= (idx % 2 == 0) ? 'post_content' : 'noclass' %>">
            <td><%=l hist.created_at, format: :default %></td>
            <td></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>