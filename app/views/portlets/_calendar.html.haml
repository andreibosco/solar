#block_schedule.block_small
  .block_title
    %h2
      %i.icon-calendar
      = t(:mysolar_agenda)
  .calendar#agenda{:url_for => (url_for :schedules)}
  .calendar_events

:javascript
  //--------------------------------------------
  // Todas as datas que possuem eventos
  //--------------------------------------------
  var dates_with_events = eval(#{@scheduled_events});
  $(function() {
    showAgenda(dates_with_events);
  });